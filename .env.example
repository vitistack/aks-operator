DEVELOPMENT=true
LOG_LEVEL=info
LOG_JSON=false
LOG_COLORIZE_LINE=true
LOG_ADD_CALLER=true
LOG_UNESCAPED_MULTILINE=true
LOG_DISABLE_STACKTRACE=false
TENANT_CONFIGMAP_NAME=aks-tenant-config
TENANT_CONFIGMAP_NAMESPACE=default
TENANT_CONFIGMAP_DATA_KEY=config.yaml
NAME_KUBERNETES_PROVIDER=aks-provider

# Azure credentials - Get these from your service principal
# See README.md for detailed instructions on creating a service principal
AZURE_SUBSCRIPTION_ID=<your-azure-subscription-id>
AZURE_TENANT_ID=<your-azure-tenant-id>
AZURE_CLIENT_ID=<your-service-principal-app-id>
AZURE_CLIENT_SECRET=<your-azure-client-secret>

# Optional: ID of the secret in Azure AD (for tracking which secret is in use)
# AZURE_SECRET_ID=<your-azure-secret-id>

# Optional: Validate Azure connectivity on startup (default: true)
# AZURE_VALIDATE_CONNECTIVITY=true
# AZURE_DEFAULT_LOCATION=westeurope