DEVELOPMENT=true
LOG_LEVEL=info
LOG_JSON=false
LOG_COLORIZE_LINE=true
LOG_ADD_CALLER=true
LOG_UNESCAPED_MULTILINE=true
LOG_DISABLE_STACKTRACE=false
TENANT_CONFIGMAP_NAME=aks-tenant-config
TENANT_CONFIGMAP_NAMESPACE=default
TENANT_CONFIGMAP_DATA_KEY=config.yaml
NAME_KUBERNETES_PROVIDER=aks-provider

# Azure User Credentials (Azure CLI) Authentication
# Use this when you have Contributor role and want to use your personal Azure login
# See docs/azure-user-credentials.md for details

# Required: Your Azure subscription ID
# Get it with: az account show --query id -o tsv
AZURE_SUBSCRIPTION_ID=

# DO NOT SET these variables when using Azure CLI authentication
# The SDK will automatically use your 'az login' credentials
# AZURE_TENANT_ID=
# AZURE_CLIENT_ID=
# AZURE_CLIENT_SECRET=
