DEVELOPMENT=true
LOG_LEVEL=info
LOG_JSON=false
LOG_COLORIZE_LINE=true
LOG_ADD_CALLER=true
LOG_UNESCAPED_MULTILINE=true
LOG_DISABLE_STACKTRACE=false
TENANT_CONFIGMAP_NAME=aks-tenant-config
TENANT_CONFIGMAP_NAMESPACE=default
TENANT_CONFIGMAP_DATA_KEY=config.yaml
NAME_KUBERNETES_PROVIDER=aks-provider

# Azure Service Principal Authentication
# Use this for production, CI/CD, or shared environments
# See docs/azure-service-principal.md for setup instructions

# Required: Your Azure subscription ID
# Get it with: az account show --query id -o tsv
AZURE_SUBSCRIPTION_ID=

# Required: Azure AD tenant ID
# Get it with: az account show --query tenantId -o tsv
AZURE_TENANT_ID=

# Required: Service principal application (client) ID
# This is the 'appId' from az ad sp create-for-rbac output
AZURE_CLIENT_ID=

# Required: Service principal client secret
# This is the 'password' from az ad sp create-for-rbac output
# WARNING: This secret is only shown once when created!
AZURE_CLIENT_SECRET=
